{
  "logs": [
    {
      "path": "test_logs/app1.log",
      "name": "app1"
    },
    {
      "path": "test_logs/app2.log",
      "name": "app2"
    }
  ],
  "patterns": {
    "error": "error|fail(ed|ure)",
    "exception": "exception",
    "fatal": "fatal",
    "warning": "warning",
    "critical": "critical"
  },
  "exclude": [
    "deprecated",
    "debug"
  ]
}
